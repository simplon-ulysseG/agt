<article id="post-7365" class="animation-cta animation-cta-b cta theme-primary  agence-type post-7365 type-agence-type status-publish has-post-thumbnail hentry" itemscope itemtype="http://schema.org/Article"
	data-animation="off"
	data-ajax-offset=1	>
		<input type="hidden" name="hidden_url_img" value="http://www.chabe.fr/wp-content/uploads/2016/02/geneve-cta-normal.jpg" class="hidden_url_img">
	<figure class="bg">

		
	  		<img  src="http://www.chabe.fr/wp-content/uploads/2016/02/geneve-cta-normal.jpg" alt="Geneva" title="Geneva" width="245" height="300"/>


	</figure> 

	<div class="content center-element" 
		data-center-offset='0'
		data-center-target='center hover'>
		
		<!-- <span class="date dn"><time pubdate datetime="2016-02-22"></time></span>	 -->

